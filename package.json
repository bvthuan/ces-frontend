{
  "name": "ces",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/viettrannetc/ces-frontend"
  },
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack-dev-server --mode development --open",
    "webpack-dev": "cross-env NODE_ENV=development webpack --mode development --open",
    "webpack-prod": "cross-env NODE_ENV=production webpack --mode production",
    "build:dev": "webpack-dev-server --inline --hot --progress --port 8080",
    "build:prod": "npm run build:clean && ngc && npm run webpack-prod && npm run build:clean",
    "build:clean": "del-cli 'src/**/*.js' 'src/**/*.js.map' 'src/**/*.ngsummary.json' 'src/**/*.metadata.json' 'src/**/**/*.ngfactory.ts' 'src/**/*.ngstyle.ts' 'src/**/*.shim.ts'"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~7.2",
    "@angular/common": "~7.2",
    "@angular/compiler": "~7.2",
    "@angular/core": "~7.2",
    "@angular/forms": "~7.2",
    "@angular/platform-browser": "~7.2",
    "@angular/platform-browser-dynamic": "~7.2",
    "@angular/router": "~7.2",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.5",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7",
    "primeng": "^6.0.0",
    "rxjs": "^6.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@types/node": "^10.0.4",
    "angular2-template-loader": "^0.6.2",
    "angular-router-loader": "~0.8",
    "awesome-typescript-loader": "~5.2",
    "html-loader": "~0.5",
    "html-webpack-plugin": "^3.2.0",
    "copy-webpack-plugin": "^4.3.1",
    "raw-loader": "^0.5.1",
    "ts-loader": "^4.3.0",
    "typescript": "~3.1",
    "webpack": "~4.29",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "~4.2",
    "del-cli": "~1.1",
    "sass-loader": "~7.1",
    "css-loader": "~1.0",
    "node-sass": "~4.12",
    "style-loader": "~0.23",
    "to-string-loader": "~1.1",
    "clean-webpack-plugin": "~0.1",
    "optimize-css-assets-webpack-plugin": "~5.0",
    "file-loader": "^3.0.1",
    "@ngtools/webpack": "~7.3",
    "cross-env": "~5.2"
  }
}
